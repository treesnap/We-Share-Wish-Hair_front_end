    userInfo 받아오기

    private void infoRequest(String accessToken) {
        String infoUrl = UrlConst.URL + "/api/user/home_info";
        JsonObjectRequest infoRequest = new JsonObjectRequest(Request.Method.GET, infoUrl, null, response -> {
            Intent intent = new Intent(FaceResultActivity.this, MainActivity.class);
            try {
                String userNickName = response.getString("nickname");
                boolean hasFaceShape = response.getBoolean("hasFaceShape");
                String faceShapeTag = response.getString("faceShapeTag");

                intent.putExtra("nickname", userNickName);
                intent.putExtra("hasFaceShape", hasFaceShape);
                intent.putExtra("faceShapeTag", faceShapeTag);
            } catch (JSONException e) {
                e.printStackTrace();
            }
            startActivity(intent);
            finish();
        }, error -> {}) {
            @Override
            public Map<String, String> getHeaders() {
                Map<String, String>  params = new HashMap();
                params.put("Authorization", "bearer" + accessToken);
                return params;
            }
        };
        RequestQueue queue = Volley.newRequestQueue(this);
        queue.add(infoRequest);
    }
